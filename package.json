{
  "name": "obsidian-poker",
  "version": "1.1.0",
  "description": "Renders poker hands in Obsidian.",
  "main": "dist/main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "fix": "npm run fix:format && npm run fix:lint",
    "fix:format": "prettier --write **/*.{mjs,ts}",
    "fix:lint": "eslint --fix **/*.{mjs,ts}",
    "test": "npm run test:types && npm run test:format && npm run test:lint",
    "test:types": "tsc -noEmit -skipLibCheck",
    "test:format": "prettier --check **/*.{mjs,ts}",
    "test:lint": "eslint **/*.{mjs,ts}",
    "build": "node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@codemirror/language": "^6.8.0",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-svg": "^0.1.0",
    "obsidian": "latest",
    "prettier": "^2.8.8",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
